{
  "name": "neo-ottawa-projet",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.tsx",
  "scripts": {
    "start": "npm run -s dev",
    "build": "npm run build:client && npm run build:ssr && npm run build:static && rm -rf dist/server",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/ssr.tsx --outDir dist/server",
    "build:static": "node prerender",
    "preview": "vite preview",
    "dev": "vite --port 4002",
    "lint": "eslint src/ --ext .tsx,.ts,.jsx,.js",
    "predeploy": "rm -rf dist && npm run build",
    "deploy": "sh scripts/deploy.sh",
    "generate:expressions": "node ./scripts/generateLoadedExpressions.js",
    "generate:assets": "npm run generate:expressions && node ./scripts/generatePNGComponents.js",
    "generate:assets:svg": "node ./scripts/generateLoadedExpressions.js && node ./scripts/generateSVGComponents.js",
    "clean:expressions": "git clean -f src/components/__generated__/characters"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minivera/neo-ottawa-projet.git"
  },
